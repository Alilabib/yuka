{
  "name": "yuka",
  "version": "0.0.1",
  "description": "JavaScript Game AI library",
  "repository": "Mugen87/yuka",
  "main": "build/yuka.js",
  "module": "build/yuka.modules.js",
  "scripts": {
    "build": "rollup -c",
    "build-uglify": "rollup -c && uglifyjs build/yuka.js > build/yuka.min.js",
    "dev": "concurrently \"rollup -c -w\" \"http-server\"",
    "lint": "eslint src examples --ext .js",
    "start": "npm run dev",
    "test": "nyc mocha test/**/*.js",
		"travis": "npm run lint && npm test"
  },
  "keywords": [
    "javascript",
    "ai"
  ],
  "eslintConfig": {
    "extends": "mdcs"
  },
  "eslintIgnore": [
    "yuka.module.js",
    "three.module.js"
  ],
  "directories": {
    "example": "examples"
  },
  "author": "Mugen87",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mugen87/yuka/issues"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "chai": "4.1.2",
    "concurrently": "3.6.1",
    "eslint": "5.1.0",
    "eslint-config-mdcs": "4.2.3",
    "http-server": "0.11.1",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "rollup": "0.63.3",
    "rollup-watch": "4.3.1",
    "uglify-es": "3.3.9"
  }
}
