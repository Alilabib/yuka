{
  "name": "yuka",
  "version": "0.0.1",
  "description": "JavaScript Game AI library",
  "repository": "Mugen87/yuka",
  "main": "build/yuka.js",
  "module": "build/yuka.modules.js",
  "scripts": {
    "build": "rollup -c",
    "build-uglify": "rollup -c && uglifyjs build/yuka.js > build/yuka.min.js",
    "dev": "rollup -c -w",
    "lint": "eslint src examples --ext .js",
    "start": "http-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "javascript",
    "ai"
  ],
  "eslintConfig": {
    "extends": "mdcs"
  },
  "eslintIgnore": [
    "yuka.module.js",
    "three.module.js"
  ],
  "directories": {
    "example": "examples"
  },
  "author": "Mugen87",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mugen87/yuka/issues"
  },
  "devDependencies": {
    "eslint": "5.1.0",
    "eslint-config-mdcs": "4.2.3",
    "http-server": "0.11.1",
    "rollup": "0.63.3",
    "rollup-watch": "4.3.1",
    "uglify-es": "3.3.9"
  }
}
